{
  "name": "jcc2d",
  "version": "0.2.4",
  "description": "一个轻量级的canvas渲染引擎",
  "main": "build/jcc2d.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "release": "npm run build && gulp release",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "preversion": "npm run release && git add -A && git commit -m 'release new version'",
    "postversion": "git push origin master && git push origin v$npm_package_version",
    "docs": "jsdoc -c scripts/jsdoc.conf.json -R README.md",
    "test": "testem ci"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jasonChen1982/jcc2d.git"
  },
  "author": "jasonChen",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.2.0",
    "eslint": "^3.8.1",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "jaguarjs-jsdoc": "^1.0.1",
    "jsdoc": "^3.4.2",
    "mocha": "^3.1.0",
    "require-dir": "^0.3.0",
    "rollup": "^0.36.3",
    "rollup-watch": "^2.5.0",
    "run-sequence": "^1.1.2",
    "testem": "^1.13.0"
  },
  "dependencies": {}
}
